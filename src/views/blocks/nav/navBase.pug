include /blocks/nav/navLogo
include /blocks/nav/navMenu
include /blocks/nav/navAddition2

mixin navBase(additionalClass, additionMobile)
	+b.fixed
		+b.nav#js-navigation&attributes(attributes)(class=additionalClass)
			+e.container.container
				+e.row.row
					+navLogo()
					+navAddition2(additionMobile)